@article{2eb9a7ffc6c94c2bad95989c71e20d06,
  author = {Maldacena, J. and Susskind, L.},
  title = {Cool horizons for entangled black holes},
  journal = {Fortschritte der Physik},
  volume = {61},
  number = {9},
  pages = {781-811},
  year = {2013},
  date = {2013-08-01},
  doi = {10.1002/prop.201300020},
  url = {https://doi.org/10.1002/prop.201300020},
  abstract = {Maldacena and Susskind argue that quantum entanglement (EPR) and non-traversable wormholes
    (Einstein–Rosen bridges) are two descriptions of the same underlying relation—“ER=EPR.” Starting from the
    thermofield-double picture of two maximally entangled black holes, they show that the classical geometry contains
    an ER bridge linking the horizons, yet remains non-traversable, so no superluminal signaling occurs. They then
    recast the AMPS firewall paradox in this setting: operations that distill or manipulate Hawking-partner quanta
    outside one hole correspond to dramatic, but still causally consistent, changes behind the other hole’s horizon.
    In this view, apparent paradoxes stem from trying to ascribe simultaneous, incompatible descriptions to entangled
    subsystems. The paper illustrates how interior experience for an infaller can remain smooth while exterior
    observers perform complicated, computationally hard manipulations on the entangled partner, and it frames
    black-hole interiors as state-dependent constructs tied to patterns of entanglement. Broadly, ER=EPR suggests
    spacetime connectivity is a manifestation of entanglement structure, linking quantum information to geometry and
    offering a route to resolve firewall-type tensions without violating causality.},
  keywords = {article},
  _id = {2eb9a7ff-c6c9-4c2b-ad95-989c71e20d06},
  _type = {article},
  created = {2025-10-24T01:10:31Z},
  modified = {2025-10-24T02:08:21Z},
  source = {doi.org},
  verified = {true},
  verified_by = {Sam Caldwell}
}

