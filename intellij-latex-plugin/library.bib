}

@misc{17e0dfcc786a419584934eeae3357973,
  author = {Matthews, P. and Rosenberg, J. and Mahy, R.},
  title = {Traversal Using Relays around NAT (TURN): Relay Extensions to Session Traversal Utilities for NAT (STUN)},
  year = {2010},
  date = {2010-04-01},
  doi = {10.17487/RFC5766},
  url = {https://www.rfc-editor.org/info/rfc5766},
  abstract = {If a host is located behind a NAT, then in certain situations it can be impossible for that host to
    communicate directly with other hosts (peers). In these situations, it is necessary for the host to use the
    services of an intermediate node that acts as a communication relay. This specification defines a protocol, called
    TURN (Traversal Using Relays around NAT), that allows the host to control the operation of the relay and to
    exchange packets with its peers using the relay. TURN differs from some other relay control protocols in that it
    allows a client to communicate with multiple peers using a single relay address.},
  keywords = {ietf, rfc},
  _id = {17e0dfcc-786a-4195-8493-4eeae3357973},
  _type = {rfc},
