@misc{bdc4a446813045f29f9bc93c899430e2,
  author = {Hao, F.},
  title = {Schnorr Non-interactive Zero-Knowledge Proof},
  year = {2017},
  date = {2017-09-01},
  doi = {10.17487/RFC8235},
  url = {https://www.rfc-editor.org/info/rfc8235},
  abstract = {This document describes the Schnorr non-interactive zero-knowledge (NIZK) proof, a non-interactive
    variant of the three-pass Schnorr identification scheme. The Schnorr NIZK proof allows one to prove the knowledge
    of a discrete logarithm without leaking any information about its value. It can serve as a useful building block
