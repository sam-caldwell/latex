@article{ec872efd9aa34b48b2c4d66f90b857de,
  author = {Grover, L. K.},
  title = {A fast quantum mechanical algorithm for database search},
  journal = {Proceedings of the twenty-eighth annual ACM symposium on Theory of computing - STOC '96},
  pages = {212-219},
  year = {1996},
  doi = {10.1145/237814.237866},
  url = {https://doi.org/10.1145/237814.237866},
  abstract = {Lov K. Grover’s 1996 STOC paper introduces a quantum search algorithm that locates a single
    “marked” item in an unstructured database of size N using O(√N) oracle queries, versus Θ(N) classically.
    The algorithm prepares a uniform superposition over N indices and iterates two unitary steps: a phase flip of the
    marked state(s) via an oracle and an inversion about the mean (the diffusion operator). Each Grover iteration
    coherently amplifies the marked amplitude, producing a rotation in a two-dimensional subspace spanned by the
    marked state and the average of unmarked states. After approximately (π/4)√N iterations, measuring the register
    returns the solution with high probability. The paper proves the query complexity is within a constant factor of
    optimal for this problem and sketches extensions to multiple matches, where about O(√(N/M)) iterations suffice
    when M items are marked. Conceptually, the speedup arises from constructive and destructive interference shaped by
    simple reflections, not massive parallelism alone. Although idealized—assuming a black-box oracle and noiseless
    unitaries—the result established a canonical limit for unstructured search and seeded a broad family of
    amplitude-amplification techniques used across quantum algorithms. Grover’s construction remains a cornerstone
    example demonstrating provable quadratic advantage under minimal structure assumptions, complementing Shor’s
    exponential-speedup results for factoring.},
  keywords = {algorithms, computer science, database, programming, quantum computing, quantum mechanics},
  _id = {ec872efd-9aa3-4b48-b2c4-d66f90b857de},
  _type = {article},
  created = {2025-10-24T01:10:31Z},
  modified = {2025-10-24T02:07:44Z},
  source = {doi.org},
  verified = {true},
  verified_by = {Sam Caldwell}
}

